# OpenCloud Configuration
# Copy this file to .env and fill in your actual values

# Domain for OpenCloud (without https://)
OCIS_DOMAIN=cloud.melomario.com

# Admin password (GENERATE A STRONG PASSWORD!)
OCIS_ADMIN_PASSWORD=change-me-to-strong-password

# Secrets (GENERATE RANDOM SECRETS!)
# Use: openssl rand -base64 32
OCIS_JWT_SECRET=change-me-to-random-secret
OCIS_TRANSFER_SECRET=change-me-to-random-secret
OCIS_MACHINE_AUTH_API_KEY=change-me-to-random-secret

# Timezone
TZ=Europe/Rome
